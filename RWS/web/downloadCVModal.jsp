<!--Download CV Modal-->
<div class="modal fade" id="downloadCVModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-body">
                <p>Are you sure you want to download the selected file(s)?</p>
            </div>
            <div class="modal-footer">
                <button id="download-submit" type="submit" class="btn btn-primary">Yes</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
            </div>
        </div>
    </div>
</div>